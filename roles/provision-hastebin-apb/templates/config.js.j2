{

  "host": "0.0.0.0",
  "port": {{ hastebin_port }},

  "keyLength": {{ key_length }},

  "maxLength": {{ max_length }},

  "staticMaxAge": 86400,

  "recompressStaticAssets": true,

  "logging": [
    {
      "level": "verbose",
      "type": "Console",
      "colorize": true
    }
  ],

  "keyGenerator": {
    "type": "phonetic"
  },

  "rateLimits": {
    "categories": {
      "normal": {
        "totalRequests": 500,
        "every": 60000
      }
    }
  },

  "storage": {
    "type": "memcached",
    "host": "0.0.0.0",
    "port": 11211
  }
}
